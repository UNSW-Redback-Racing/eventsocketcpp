get_filename_component(EventSocketCpp_CMAKE_DIR "${CMAKE_SOURCE_DIR}" PATH)
include(CMakeFindDependencyMacro)

list(APPEND CMAKE_MODULE_PATH ${EventSocketCpp_CMAKE_DIR})

@PACKAGE_INIT@
if(NOT TARGET EventSocketCpp::EventSocketCpp)
		message(STATUS ${CMAKE_INSTALL_LIBDIR})
		include("${CMAKE_INSTALL_LIBDIR}/cmake/EventSocketCppTargets.cmake")
endif()

set(EVENTSOCKETCPP_LIBRARIES EventSocketCpp::EventSocketCpp)
